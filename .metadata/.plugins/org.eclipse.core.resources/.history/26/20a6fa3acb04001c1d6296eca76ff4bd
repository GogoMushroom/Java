package airlineReservationSystem;

public class main {
	public static void main(String[] args) {
		
		
		Reservation r = new Reservation();
		r.Reserve();
		
		
		SSS s = new SSS();
		s.GoPlane();
		s.BackPlane();
		
		AddExtra ae= new AddExtra();
		ae.addFood();
		ae.addBaggage();
		ae.total();
	}
}
