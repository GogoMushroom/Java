package airlineReservationSystem;

public class TotalPlaneInfo {
	String ResInfo = (Reservation.startplace +"->"+ Reservation.arrplace);
	String SSSInfo = ("가는편비행기"+SSS2.Go+"\n"+"오는편비행기"+SSS2.Back);
	String ExtraInfo =("기내식"+AddExtra.Food+"\n추가 수화물"+AddExtra.Baggage); 
	String SeatUpInfo = ("좌석 업그레이드"+Seat.SeatUp);
	
	String totalInfo = (ResInfo+SSSInfo+ExtraInfo+SeatUpInfo);
			
	void total() {
	System.out.println(totalInfo);
}
}